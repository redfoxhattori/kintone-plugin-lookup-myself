{
  "name": "kintone-plugin-lookup-myself",
  "version": "2.1.0",
  "author": "ribbit",
  "license": "MIT",
  "scripts": {
    "create-ppk": "kintone-plugin-packer --out dist/plugin-dev.zip plugin",
    "move-ppk": "node scripts/move-ppk.ts",
    "init": "run-s develop create-ppk move-ppk",
    "bundle:dev": "webpack --config webpack.dev.js",
    "bundle:prod": "webpack --config webpack.prod.js",
    "bundle:watch": "webpack --config webpack.dev.js -w",
    "merge-manifest:dev": "ts-node scripts/marge-json.ts dev",
    "merge-manifest:prod": "ts-node scripts/marge-json.ts prod",
    "develop": "run-s merge-manifest:dev bundle:dev",
    "build": "run-s  merge-manifest:prod bundle:prod",
    "watch": "run-s merge-manifest:dev bundle:watch",
    "upload": "ts-node scripts/upload.ts",
    "start": "run-p watch upload",
    "lint": "eslint ./src --cache --ext .js,.ts,.tsx"
  },
  "devDependencies": {
    "@emotion/css": "11.10.6",
    "@emotion/styled": "11.10.6",
    "@kintone/dts-gen": "6.1.24",
    "@kintone/plugin-packer": "6.0.30",
    "@kintone/plugin-uploader": "7.1.10",
    "@kintone/rest-api-client": "3.3.7",
    "@kintone/webpack-plugin-kintone-plugin": "6.0.30",
    "@mui/icons-material": "5.11.11",
    "@mui/material": "5.11.15",
    "@types/dompurify": "2.4.0",
    "@types/luxon": "3.2.0",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.55.0",
    "@typescript-eslint/parser": "5.55.0",
    "dompurify": "2.4.5",
    "dotenv": "16.0.3",
    "esbuild-loader": "3.0.1",
    "eslint": "8.36.0",
    "fork-ts-checker-webpack-plugin": "7.3.0",
    "immer": "9.0.19",
    "luxon": "3.3.0",
    "notistack": "2.0.8",
    "npm-run-all": "4.1.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "recoil": "0.7.7",
    "terser-webpack-plugin": "5.3.7",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "webpack": "5.76.2",
    "webpack-cli": "5.0.1",
    "webpack-merge": "5.8.0"
  }
}
